<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mu.android.rssplus" >

    <uses-permission android:name="android.permission.INTERNET" />

    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:normalScreens="true"
        android:resizeable="true"
        android:smallScreens="true" />

    <application
        android:icon="@drawable/icon"
        android:label="@string/app_name" >
        <activity android:name="com.mu.android.rssplus.FeedSelection" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mu.android.rssplus.Reader" >
        </activity>
        <activity android:name="com.mu.android.rssplus.Description" >
        </activity>
        <activity android:name="com.mu.android.rssplus.Browser" >
        </activity>
    </application>

</manifest>